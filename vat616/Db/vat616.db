record (longin, "$(P)$(V):sErrCode") {
    field(SCAN, "5 second")
    field(DTYP, "stream")
    field(INP, "@vat616.proto get_data($(INDX),$(P)$(V):sErrNum,$(P)$(V):sAccMode,$(P)$(V):sSwitches,$(P)$(V):sRRdV) $(PORT)")
    field(LOW, -1)
    field(LSV, "MINOR")
    field(HIHI, 1)
    field(HHSV, "MAJOR")
}

record (longin, "$(P)$(V):sErrNum") {
    field(DISV, "-1")
#    field(SDIS, "$(P)$(V):sErrCode.VAL")
#    field(DISS, "INVALID")
    field(HIHI, 1)
    field(HHSV, "MAJOR")
}

record (bi, "$(P)$(V):sAccMode") {
    field(ZNAM, "Local")
    field(ONAM, "Remote")
    field(ZSV, "MAJOR")
}

record (mbbi, "$(P)$(V):sSwitches") {
    field(ZRVL, "0")
    field(ONVL, "1")
    field(TWVL, "2")
    field(ZRST, "Indeterm.")
    field(ONST, "Closed")
    field(TWST, "Open")
#    field(HIHI, 1)
#    field(HHSV, "MAJOR")
}

record (ai, "$(P)$(V):sRRdV") {
    field(EGU, "%")
}


record (bo, "$(P)$(V):cmdRst") {
    field(DTYP, "stream")
    field(OUT, "@vat616.proto snd_cmd($(INDX), R) $(PORT)")
    field(UDFS, "NO_ALARM")
}
record (bo, "$(P)$(V):cmdRem") {
    field(DTYP, "stream")
    field(OUT, "@vat616.proto snd_cmd($(INDX), Rem) $(PORT)")
    field(UDFS, "NO_ALARM")
}

record (bo, "$(P)$(V):cmdLoc") {
    field(DTYP, "stream")
    field(OUT, "@vat616.proto snd_cmd($(INDX), Loc) $(PORT)")
    field(UDFS, "NO_ALARM")
}

record (bi, "$(P)$(V):sErr-cmd") {
    field(SCAN, "I/O Intr")
    field(DTYP, "stream")
    field(INP, "@vat616.proto cmd_stat($(INDX)) $(PORT)")
    field(VAL, "0")
    field(ZNAM, "OK")
    field(ONAM, "ERR")
    field(ZSV, "NO_ALARM")
    field(OSV, "MAJOR")
    field(PINI, "YES")
}
